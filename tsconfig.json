{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@auth/*": ["src/modules/auth/*"],
      "@users/*": ["src/modules/users/*"],
      "@orders/*": ["src/modules/orders/*"],
      "@shared/*": ["src/shared/*"],
      "@admin/*": ["src/modules/admin/*"],
      "@chat/*": ["src/modules/chat/*"],
      "@config/*": ["src/config/*"],
      "@database/*": ["./prisma/*"],
      "@prisma/*": ["./database/generated/prisma/*"],
      "@utils/*": ["src/utils/*"],
      "@test/*": ["src/test/*"],
      "@onboarding/*": ["src/modules/onboarding/*"],
      "@kyc/*": ["src/modules/kyc/*"],
      "@inventory/*": ["src/modules/inventory/*"],
      "@geolocation/*": ["src/modules/geolocation/*"],
      // "@prisma/*": ["./prisma/*"],
      "@reports/*": ["src/modules/reports/*"],
      "@audit/*": ["src/modules/audit/*"],
      "@products/*": ["src/modules/products/*"],
      "@payments/*": ["src/modules/payments/*"],
      "@delivery/*": ["src/modules/delivery/*"],
      "@notifications/*": ["src/modules/notifications/*"],
      "@marketplace/*": ["src/modules/marketplace/*"],
      "@loyalty/*": ["src/modules/loyalty/*"],
      "@search/*": ["src/modules/search/*"],
      "@scheduling/*": ["src/modules/scheduling/*"],
      "@errands/*": ["src/modules/errands/*"],
      "@drivers/*": ["src/modules/drivers/*"],
      "@common/*": ["src/modules/common/*"],
      "@auth/domain/*": ["src/modules/auth/domain/*"],
      "@auth/application/*": ["src/modules/auth/application/*"],
      "@auth/infrastructure/*": ["src/modules/auth/infrastructure/*"],
      "@auth/interfaces/*": ["src/modules/auth/interfaces/*"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false
  }
}
